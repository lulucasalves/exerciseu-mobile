{
  "expo": {
    "name": "exerciseu",
    "slug": "ExerciseU",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "Exerciseu",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#508BF5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#508BF5"
      },
      "package": "com.linux.exerciseu"
    },
    "androidStatusBar": {
      "barStyle": "light-content"
    },
    "platforms": ["ios", "android"]
  }
}
